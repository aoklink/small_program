<view class="{{act ? 'uuu' : 'ppp'}}" bindtap='add'> Hello {{name}}! </view> <button bindtap="changeName"> Click me! </button>
<!--wxml-->
<template name="staffName">
  <view> FirstName: {{firstName}}, LastName: {{lastName}} </view>
</template>

<template is="staffName" data="{{...staffA}}"></template>
<template is="staffName" data="{{...staffB}}"></template>
<template is="staffName" data="{{...staffC}}"></template>
<view style='display:inline-flex;background:red'>
  <view class='yy' wx:for="{{jj}}"> {{item}} </view>
</view>
<view bindtap='yzy'>yzy</view>
<view class="container">
    <view bindtap="gotoPage" data-page="line" class="mt20 list-item">Line</view>
    <view bindtap="gotoPage" data-page="scrollline" class="mt20 list-item">ScrollLine</view>
    <view bindtap="gotoPage" data-page="column" class="list-item">Column</view>
    <view bindtap="gotoPage" data-page="pie" class="list-item">Pie</view>
    <view bindtap="gotoPage" data-page="ring" class="list-item">Ring</view>
    <view bindtap="gotoPage" data-page="area" class="list-item">Area</view>
    <view bindtap="gotoPage" data-page="radar" class="list-item">Radar</view>
</view>
