
.section1 {
  margin-top: 20rpx;
  background-color: #fff;
  display: flex; 
  justify-content: space-around;
  border-bottom:1rpx solid #f2f2f2; 
}

.section1-item {
  display: flex;
  flex-direction: column;
}

.item1-title {
  font-size: 14px;
  padding-top: 20rpx;
}

.item1-icon {
  background-color: #e5e5e5;
  height: 20rpx;
  width: 20rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  align-self: center;

}

.section2 {
  display: flex;
  flex-direction: column;
  background-color: #fff; 
  border-bottom:1rpx solid #f2f2f2;  
  font-size: 12px;
}

.section2-item {
  display: flex;
  flex-direction: row;
  padding: 30rpx 10rpx 30rpx 10rpx;
  border-bottom:1rpx solid #f2f2f2; 
 
}

.item2-left {
  width: 50%;
}

.item2-right {
  width: 50%;
}

.load_more {
  width: 65%;
  margin: 20rpx auto;
  font-size: 14px;
  text-align: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.load_loading {
  margin: 0 5px;
  width: 40rpx;
  height: 40rpx;
  /* display: inline-block;
  vertical-align: middle; */
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.load_tips {
  display: inline-block;
  vertical-align: middle;
}

.load_end {
  width: 65%;
  margin: 20rpx auto;
  font-size: 14px;
  text-align: center;
}
