}
.impotb{
  margin: 62rpx auto 0;
  width: 100%;
}
.up{
  display: inline-block;
  margin: auto;
  width: 150rpx;
  height: 34rpx;
  position: relative;
}
.smstu{
  display: inline-block;
  background: url(http://ll.linkfeeling.cn/img/os.png) center no-repeat;
  border-radius: 50%;
  width: 26rpx;
  position: absolute;
  height: 26rpx;
  top: 4rpx;
  left: 0rpx;
}
.smstit{
  position: absolute;
  left: 40rpx;
  display: inline-block;
  height: 34rpx;
  min-width: 100rpx;
  font-size: 24rpx;
  line-height: 34rpx;
}
.lin{
  width: 100%;
  height: 34rpx;
  margin: 24rpx auto 0;
  line-height: 34rpx;
  font-size: 24rpx;
}
.fou{
  width: 100%;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 24rpx;
}
