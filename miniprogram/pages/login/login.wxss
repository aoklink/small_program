.headerhh {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 
.headerhh image {
    width: 200rpx;
    height: 200rpx;
}
 
.contenthh {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
 
.contenthh text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottomhh {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}
